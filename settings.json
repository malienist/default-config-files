{
    "workbench.iconTheme": "material-icon-theme",
    "breadcrumbs.enabled": false,
    "editor.detectIndentation": false,
    "workbench.editor.enablePreview": false,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.cursorStyle": "line",
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.cursorStyle": "line",
    },
    "bracketPairColorizer.showHorizontalScopeLine": false,
    "prettier.tabWidth": 4,
    "prettier.singleQuote": true,
    "prettier.useTabs": true,
    "editor.formatOnPaste": true,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.cursorStyle": "line",
    },
    "window.zoomLevel": 0,
    "workbench.startupEditor": "newUntitledFile",
    "explorer.confirmDelete": false,
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.cursorStyle": "line",
    },
    "terminal.integrated.fontSize": 11,
    "debug.console.fontSize": 11,
    "codemetrics.basics.EnabledForHTML": false,
    "codemetrics.basics.EnabledForLua": false,
    "codemetrics.basics.OverviewRulerModeEnabled": false,
    "bracketPairColorizer.consecutivePairColors": [
        "()",
        "[]",
        "{}",
        [
            "Gold",
            "Orchid",
            "LightSkyBlue"
        ],
        "Red"
    ],
    "spotify.showLyricsButton": false,
    "spotify.lyricsButtonPriority": -1,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.cursorStyle": "underline",
    },
    "editor.tokenColorCustomizations": null,
    "editor.rulers": [],
    "editor.formatOnType": true,
    "editor.quickSuggestions": true,
    "color-highlight.markRuler": false,
    "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": true,
    "workbench.list.smoothScrolling": true,
    "debug.javascript.usePreview": true,
    "editor.linkedEditing": true,
    "scss.lint.duplicateProperties": "warning",
    "scss.lint.float": "warning",
    "scss.lint.zeroUnits": "warning",
    "liveServer.settings.donotShowInfoMsg": true,
    "editor.fontSize": 12,
    "workbench.activityBar.visible": true,
    "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "codemetrics.basics.DecorationTemplate": "<svg xmlns='http://www.w3.org/2000/svg' width='{{size}}px' height='{{size}}px' viewbox='0 0 {{size}} {{size}}'><rect width='{{size}}' height='{{size}}' style='fill:{{color}};stroke-width:1;stroke:{{color}}'/></svg>",
    "files.associations": {
        "*.scss": "scss",
        "*.gs": "javascript"
    },
    "[scss]": {
        "editor.autoIndent": "full",
        "editor.suggest.insertMode": "replace",
        "editor.cursorStyle": "line",
        "editor.formatOnPaste": false,
    },
    "codemetrics.basics.CodeLensHiddenUnder": 6,
    "workbench.colorTheme": "codeSTACKr Theme",
    "editor.tabSize": 2,
    "editor.minimap.enabled": false,
    "editor.autoIndent": "full"
}
